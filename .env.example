# SQLite connection string for Prisma. In production you can switch to PostgreSQL by
# updating the provider in prisma/schema.prisma and this variable accordingly.
DATABASE_URL="file:./dev.db"

NEXTAUTH_URL="http://localhost:3000"

# A long random string used to sign tokens for NextAuth.js. Replace this with your own secret.
NEXTAUTH_SECRET="CHANGE_ME_TO_A_LONG_RANDOM_STRING"

# OAuth credentials (set these only if you enable GoogleProvider in authOptions)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
